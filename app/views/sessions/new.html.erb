<h1>Log in</h1>

<%= render "errors/errors" %>

<%= form_tag sessions_path do %>
  <%= label_tag :email %>
  <%= text_field_tag :email, "" ,class: 'form-inputs' %>
  <%= label_tag :password %>
  <%= password_field_tag :password, "" ,class: 'form-inputs' %>
  <%= submit_tag 'Log in', class: 'submit_button' %>
<% end %>
